package edu.fju.exam;

public class Bubble {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int storage = 0;
 		int[] numbers = {45, 81, 9, 72, 4, 6};
 		for (int i=0;i<=4;i++){
 			for (int j=1;j<=5;j++){
 				if(numbers[i]<numbers[j]){
 					storage = numbers[i];
 					numbers[i]=numbers[j];
 					numbers[j]=storage;
 				}
 			}
 		}
 		for (int k=0;k<numbers.length;k++){
 			System.out.print(numbers[k]+" ");
 		}
 	}
}
